<div class="contact-main">
  <%= image_tag 'menu-white.svg', alt: "Open", class: "icon-menu", id: "simple-menu", href: "#sidr" %>
  <div class="content container">
    <h1>Get the latest updates from Stories</h1>
    <div class="row">
      <div class="col-md-4">
        
          <%= form_for @signup do |s| %>
            <div class="form-group">    
              <%= s.label :firstname, "First name" %>
              <%= s.text_field :firstname, class: "form-control" %>
            </div>
            <div class="form-group">
              <%= s.label :email, "Email" %>
              <%= s.text_field :email, class: "form-control"%>
              
            </div>
            
            <%= s.submit "Join", class: "btn btn-default" %>
            
          
          <% end %>
      </div>
    </div>
  </div>
</div>